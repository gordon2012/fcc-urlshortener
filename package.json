{
  "name": "fcc-urlshortener",
  "version": "1.0.0",
  "description": "Boilerplate for Microservices",
  "main": ".build/server.js",
  "scripts": {
    "clean": "rm -rf ./.build",
    "build:server": "webpack --env.NODE_ENV=production --config webpack.config.server.js",
    "build:client": "webpack --env.NODE_ENV=production --config webpack.config.client.js",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "start:server": "webpack --config webpack.config.server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start": "npm run clean && npm-run-all --parallel start:server start:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gordon2012/fcc-urlshortener.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gordon2012/fcc-urlshortener/issues"
  },
  "homepage": "https://github.com/gordon2012/fcc-urlshortener#readme",
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.1",
    "npm-run-all": "^4.1.3",
    "react-hot-loader": "^4.3.12",
    "start-server-webpack-plugin": "^2.2.5",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  }
}
